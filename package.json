    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "bundt": "^1.1.1",
        "eslint": "^7.15.0",
        "eslint-config-byungi": "^0.14.3",
        "esm": "^3.2.25",
        "uvu": "^0.5.1",
        "watchlist": "^0.2.3"
    },
    "eslintConfig": {
        "extends": "byungi",
        "rules": {
            "quote-props": "off"
        }
    },
    "eslintIgnore": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    }
}
