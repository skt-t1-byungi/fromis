{
    "name": "@byungi/promis",
    "description": "A simple promise implementation (~300Byte)",
    "repository": "https://github.com/skt-t1-byungi/promis.git",
    "author": "byungi <tiniwz@gmail.com>",
    "version": "0.0.2",
    "source": "src/index.js",
    "main": "dist/index.js",
    "module": "dist/index.module.js",
    "unpkg": "dist/index.umd.js",
    "types": "types.d.ts",
    "license": "MIT",
    "keywords": [
        "promise",
        "async",
